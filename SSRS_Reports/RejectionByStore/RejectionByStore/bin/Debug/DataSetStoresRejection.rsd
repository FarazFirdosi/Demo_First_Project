<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>DataSource1</DataSourceReference>
      <CommandText>SELECT        Store_Acceptance.IMR_No, Store_Acceptance.IMR_Date, Store_Acceptance.Supplier_Name, Store_Acceptance.Supplier_Id, Store_Acceptance.InvoiceNo, Store_Acceptance.InvoiceDescription, Store_Acceptance.Invoice_Date, 
                         Store_Acceptance.Amount_Gross, Store_Acceptance.Accepted_By, Store_Acceptance.Acceptance_Date, Store_Acceptance.SendToProcess, Store_Acceptance.RejectionStatus, Store_Rejection.RejectedBy, 
                         Store_Rejection.RejectionDate, Store_Rejection.Remarks
FROM            Store_Acceptance INNER JOIN
                         Store_Rejection ON Store_Acceptance.IMR_No = Store_Rejection.IMR_No</CommandText>
    </Query>
    <Fields>
